{
  "name": "raula",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json && pnpm build:esbuild && pnpm build:type",
    "build:esbuild": "esbuild src/index.ts --bundle --outfile=dist/index.js --format=esm --target=esnext --loader:.ts=tsx --external:react --jsx=automatic --minify",
    "build:type": "tsc --project tsconfig.build.json",
    "test": "vitest"
  },
  "dependencies": {
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.2",
    "@types/babel__core": "^7.20.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild": "^0.17.5",
    "typescript": "^4.9.3",
    "vite": "^4.1.1",
    "vitest": "^0.28.4"
  }
}

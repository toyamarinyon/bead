{
  "extends": "@tsconfig/esm/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "strict": true,
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
  },
  "exclude": ["node_modules"]
}
